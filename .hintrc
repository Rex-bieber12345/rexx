{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "form[autocapitalize]"
        ]
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "meta-viewport": "off",
    "meta-charset-utf-8": "off",
    "button-type": "off",
    "axe/aria": [
      "default",
      {
        "aria-allowed-attr": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off"
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off"
      }
    ],
    "no-inline-styles": "off"
  }
}